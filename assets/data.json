import * as S from "@effect/schema/Schema";


export class Wind extends S.Class<Wind>("Wind")({
  "deg": S.Number,
  "speed": S.Number,
}) {}

export class Weather extends S.Class<Weather>("Weather")({
  "description": S.String,
  "icon": S.String,
  "id": S.Number,
  "main": S.String,
}) {}

export class Sys extends S.Class<Sys>("Sys")({
  "country": S.String,
  "id": S.Number,
  "sunrise": S.Number,
  "sunset": S.Number,
  "type": S.Number,
}) {}

export class Main extends S.Class<Main>("Main")({
  "feels_like": S.Number,
  "grnd_level": S.Number,
  "humidity": S.Number,
  "pressure": S.Number,
  "sea_level": S.Number,
  "temp": S.Number,
  "temp_max": S.Number,
  "temp_min": S.Number,
}) {}

export class Coord extends S.Class<Coord>("Coord")({
  "lat": S.Number,
  "lon": S.Number,
}) {}

export class Clouds extends S.Class<Clouds>("Clouds")({
  "all": S.Number,
}) {}

export class Data extends S.Class<Data>("Data")({
  "base": S.String,
  "clouds": Clouds,
  "cod": S.Number,
  "coord": Coord,
  "dt": S.Number,
  "id": S.Number,
  "main": Main,
  "name": S.String,
  "sys": Sys,
  "timezone": S.Number,
  "visibility": S.Number,
  "weather": S.Array(Weather),
  "wind": Wind,
}) {}

